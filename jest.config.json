{
	"roots": ["src/jest-test"],
	"moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "src"],
	"verbose": true,
	"collectCoverage": true,
    "collectCoverageFrom": ["./src/**"],
    "coverageThreshold": {
      "global": {
		    "statements": 100,
		    "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
}
