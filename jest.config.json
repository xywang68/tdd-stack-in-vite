{
	"roots": ["src/jest-test"],
	"moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "src"],
	"verbose": true,
	"collectCoverage": true,
    "collectCoverageFrom": ["./src/**"],
    "coverageThreshold": {
      "global": {
        "lines": 100,
		"statements": 100,
		"branches": 100
      }
    }
}
